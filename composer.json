{
    "name": "lumberjacked/BasConsole",
    "description": "Command Console to generate zf2 files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Spencer Bragg",
            "email": "sbragg.braggaboutsystems@gmail.com"
        },
        {
            "name": "James Carr",
            "email": "james.r.carr@gmail.com"
        }
    ],
    "minimum-stability":"dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "symfony/Console": "dev-master",
        "zendframework/zend-servicemanager": "2.0.*",
        "zendframework/zend-json": "2.0.*",
        "zendframework/zend-config": "2.0.*"
    },

    "autoload": {
        "psr-0": {
            "BasConsole": "src/"
        }
    },
    "config": {
        "bin-dir" : "bin"
    },
    "bin": [
        "bin/zf2"
    ]


}
